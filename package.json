{
	"name": "indelvve",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "svelte-kit sync && npm run gen",
		"gen": "npm run gen:modules && yarn gen:types",
		"gen:modules": "node ./node_modules/@kitql/module-codegen/index.js",
		"gen:types": "graphql-codegen --config ./.graphqlrc.yaml"
	},
	"prisma": {
		"seed": "ts-node script.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.25.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/bcrypt": "^5.0.0",
		"@types/node": "^18.7.18",
		"@types/quill": "^2.0.9",
		"@typescript-eslint/eslint-plugin": "^5.37.0",
		"@typescript-eslint/parser": "^5.37.0",
		"autoprefixer": "^10.4.11",
		"eslint": "^8.23.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.16",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"prisma": "^4.4.0",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.0",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.8",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.0",
		"typescript": "^4.8.3",
		"vite": "^3.1.1"
	},
	"type": "module",
	"dependencies": {
		"@editorjs/editorjs": "^2.25.0",
		"@kitql/all-in": "^0.7.10",
		"@papyrs/stylo": "^0.0.38",
		"@prisma/client": "^4.3.1",
		"bcrypt": "^5.0.1",
		"graphql": "^16.6.0",
		"houdini": "^0.16.8",
		"quill": "^1.3.7",
		"svelte-quill": "^1.2.0"
	}
}
